print("Hello, World!"import matplotlib.pyplot as plt

# Example data for the graph
days = ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']
steps = [5000, 6000, 5500, 7000, 8000, 7500, 9000]

# Create the plot
plt.plot(days, steps, marker='o')

# Set labels and title
plt.title("Weekly Fitness Progress")
plt.xlabel("Days")
plt.ylabel("Steps Taken")

# Show the plot
plt.show()
